{
  "name": "tailwind-nextjs-starter-blog",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "fmt": "prettier . --write --cache --cache-strategy=content --cache-location=.prettiercache",
    "fmt.check": "prettier . --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "analyze": "cross-env ANALYZE=true next build",
    "lint:js": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:md": "eslint \"**/*.md?(x)\" --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy=content --cache-location=.eslintmdcache",
    "lint:css": "stylelint \"**/*.css\" --max-warnings 0 --allow-empty-input --cache --cache-strategy=content --cache-location=.stylelintcache",
    "lint": "run-s lint:md lint:js lint:css",
    "playwright": "playwright"
  },
  "dependencies": {
    "@headlessui/react": "2.1.9",
    "@next/bundle-analyzer": "14.2.14",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^2.1.1",
    "contentlayer2": "0.5.1",
    "esbuild": "0.24.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "1.1.1",
    "next": "14.2.14",
    "next-contentlayer2": "0.5.1",
    "next-themes": "^0.3.0",
    "pliny": "0.3.2",
    "postcss": "^8.4.24",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-preset-minify": "7.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.3",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.5.0",
    "@eslint/compat": "^1.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@playwright/test": "^1.47.2",
    "@svgr/webpack": "^8.0.1",
    "@types/mdx": "^2.0.12",
    "@types/node": "^22.7.4",
    "@types/react": "^18.2.73",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.12.0",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.10.0",
    "gzip-size": "^7.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.10",
    "mkdirp": "^3.0.1",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.8.0"
  },
  "packageManager": "pnpm@9.12.0"
}
